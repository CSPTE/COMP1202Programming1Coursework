		System.out.println("Current KP SE1 = " + sestudent1.getKP());
		System.out.println("Current HP AI1 = " + aistudent1.getHP());
		System.out.println("Current HP CS1 = " + csstudent1.getHP());
		System.out.println("Current HP Cyber1 = " + cyberstudent1.getHP());
		System.out.println("");
		System.out.println("Current HP AI2 = " + aistudent2.getHP());
		System.out.println("Current HP CS2 = " + csstudent2.getHP());
		System.out.println("Current HP Cyber2 = " + cyberstudent2.getHP());
		System.out.println("Current HP SE2 = " + sestudent2.getHP());*/
		
		//Student on Monster
		minion1.increaseEP(20);
		//System.out.println("Minion1 level = " + minion1.level);
		//System.out.println("Minion2 level = " + minion2.level);
		//System.out.println("Boss1 Level = " + boss1.level);
		System.out.println("Minion1 HP = " + minion1.currentHP);
		System.out.println("Minion2 HP = " + minion2.currentHP);
		System.out.println("Boss1 HP = " + boss1.currentHP);
		aistudent1.increaseKP(10);
		//System.out.println("AI1, KP = HIGH, HP = HIGH");
		System.out.println("Current HP AI1 = " + aistudent1.getHP());
		System.out.println("Current EP AI1 = " + aistudent1.getEP());
		System.out.println("Current KP AI1 = " + aistudent1.getKP());
		System.out.println("");
		try {
			firstTeam.move(aistudent1, thirdTeam);
		} catch (Exception m) {
			System.out.println("Exception occured: "+m);
		}
		System.out.println("Current HP AI1 = " + aistudent1.getHP());
		System.out.println("Current EP AI1 = " + aistudent1.getEP());
		System.out.println("Current KP AI1 = " + aistudent1.getKP());
		System.out.println("");
		System.out.println("Current HP Boss1 = " + boss1.getHP());
		System.out.println("Current HP Minion1 = " + minion1.getHP());
		System.out.println("Current HP Minion2 = " + minion2.getHP());
		
		//Monster on Student
